
(rule
  ((targets (run_qtest.ml))
   (deps ((file Makefile)(files_recursively_in ../src)))
   (fallback)
   ;(libraries (qtest qcheck))
   (action
     (run make qtest-gen))
  ))

(executable
  ((name run_qtest)
   (libraries (containers nunchaku nunchaku.parsers nunchaku.transformations
               nunchaku.random oUnit qcheck))))

(alias
 ((name    runtest)
  (deps    (run_qtest.exe))
  (action  (run ${<}))
  ))

